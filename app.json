{
  "expo": {
    "name": "vyre",
    "slug": "vyre3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vyreLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/vyre.png",
      "resizeMode": "auto",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/vyreLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.harveyana.vyre"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/vyreLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "allow vyre to use Face ID."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d6ecc2c7-e500-478d-9e92-fe468cf6ee76"
      }
    }
  }
}
